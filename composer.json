    "license": "MIT",
    "authors": [
        {
            "name": "Dating Libre",
            "email": "admin@datinglibre.com"
        }
    ],
    "description": "The core overridable code of DatingLibre",
    "require": {
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.140",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "gumlet/php-image-resize": "^2.0",
        "martin-georgiev/postgresql-for-doctrine": "^1.5",
        "mlocati/ip-lib": "^1.13",
        "symfony/framework-bundle": "5.2.*",
        "symfony/flex": "^1.11",
        "symfony/uid": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "psr/container": "^1.0",
        "symfony/form": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/amazon-mailer": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/translation": "5.2.*"
    },
    "autoload": {
        "psr-4": {
            "DatingLibre\\AppBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DatingLibre\\AppBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/phpunit-bridge": "^5.2",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3.1",
        "friends-of-behat/symfony-extension": "^2.1"
    }
}
